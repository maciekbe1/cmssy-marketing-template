{
  "name": "cmssy-marketplace",
  "version": "1.0.0",
  "description": "Cmssy Marketplace - Blocks and Templates deployed to Vercel CDN",
  "type": "module",
  "scripts": {
    "build": "node scripts/build-all.js && node scripts/prepare-cdn.js",
    "build:block": "node scripts/build-block.js",
    "prepare-cdn": "node scripts/prepare-cdn.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "check": "pnpm lint && pnpm type-check"
  },
  "keywords": [
    "cmssy",
    "blocks",
    "templates",
    "marketplace"
  ],
  "author": "Cmssy Team",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "esbuild": "^0.19.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
